/**app.wxss**/
@import "/assets/css/iconfont.wxss";
@import "/assets/css/common.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page,.page {
  width: 100%;
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  position: relative;
}

.page-content {
  /* padding-bottom: 44px;  */
  flex: 1;
  box-sizing: border-box;
}

.fb {
  font-weight: bold;
}


.trans-ease{
  transition: all .2s ease;
}