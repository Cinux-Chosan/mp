<template name="footer">
<view class="footer footer_default trans-ease">
  <view class="footer__popup  {{isOpen? 'open': ''}}">
  <view class="footer__popup__inner">
  </view>
    
  </view>
  <view class="footer__inner">
    <view class="footer__item" bindtap="chooseLocation">
      <icon class="iconfont icon-search"></icon>
    </view>
    <view class="footer__item" bindtap="resetMapCenter">
      <icon class="iconfont icon-dingwei"></icon>
    </view>
    <view class="footer__item" bindtap="exchange">
      <icon class="iconfont icon-exchange fb"></icon>
    </view>
    <view class="footer__item" bindtap="toggleMenus">
      <icon class="iconfont icon-up"></icon>
    </view>
  </view>
</view>
</template>